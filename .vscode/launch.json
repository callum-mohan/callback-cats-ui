{
    "version": "0.2.0",
    "configurations": [
    
    {
    "type": "node",
    "request": "launch",
    "name": "Launch Program",
    "skipFiles": [
    "<node_internals>/**"
    ],
    "program": "${workspaceFolder}/start",
    "preLaunchTask": "tsc-watch",
    "restart": true,
    "outFiles": [
    "${workspaceFolder}/built/**/*.js"
    ]
    }
    ]
}